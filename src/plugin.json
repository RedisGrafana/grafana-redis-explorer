      "large": "img/logo.svg",
      "small": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "Cluster Overview Dashboard",
        "path": "img/overview.png"
      },
      {
        "name": "Redis Enterprise Software Data Source",
        "path": "img/datasource.png"
      },
      {
        "name": "Enable Redis Explorer plugin",
        "path": "img/enable.png"
