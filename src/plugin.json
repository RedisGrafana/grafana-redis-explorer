    },
    {
      "addToNav": true,
      "name": "Cluster Overview",
      "path": "dashboards/cluster-overview.json",
      "role": "Admin",
      "type": "dashboard"
    },
    {
      "addToNav": true,
      "name": "Cluster Nodes",
      "icon": "sitemap",
      "path": "dashboards/cluster-nodes.json",
      "role": "Admin",
